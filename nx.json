{
  "npmScope": "grillo-software",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "stock": {
      "tags": ["app-stock"]
    },
    "stock-e2e": {
      "tags": ["e2e-stock"],
      "implicitDependencies": ["stock"]
    },
    "environments": {
      "tags": ["environments"]
    },
    "styles": {
    },
    "stock-list": {
      "tags": []
    }
  }
}
